    "@nestjs/websockets": "^11.1.12",
    "@tuya/tuya-connector-nodejs": "^2.1.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "google-auth-library": "^10.5.0",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.18.0",
    "puppeteer": "^24.36.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.16.0",
    "ts-node": "^10.9.1",
